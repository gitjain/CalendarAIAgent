{
  "name": "calendar-app",
  "version": "1.0.0",
  "description": "React web app with Express server for calendar events",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm install && npm run install-client",
    "mcp:meal-planning": "MCP_RUN_MODE=server python3 mcp-servers/meal-planning/meal_planning_server.py"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "googleapis": "^164.1.0",
    "nodemon": "^3.0.1",
    "openai": "^4.20.1"
  },
  "keywords": [
    "react",
    "express",
    "calendar",
    "mock-data"
  ],
  "author": "",
  "license": "MIT"
}
